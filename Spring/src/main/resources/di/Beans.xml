<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="textEditor" class="com.lwu.spring.denpendency.injection.beanreference.TextEditor">
        <constructor-arg index="0" value="2001"/>
        <constructor-arg index="1" value="Zara"/>
    </bean>

    <bean id="spellChecker1" class="com.lwu.spring.denpendency.injection.beanreference.SpellChecker"/>
    <bean id="spellChecker2" class="com.lwu.spring.denpendency.injection.beanreference.SpellChecker"/>
</beans>